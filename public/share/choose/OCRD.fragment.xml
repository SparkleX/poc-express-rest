<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<TableSelectDialog noDataText="No Data" title="Choose From List" search="onSearch" confirm="_onConfirm" cancel="_onClose" items="{cfl>/}">
		<ColumnListItem>
			<cells>
				<ObjectIdentifier text="{cfl>id}" />
				<Text text="{cfl>bpCode}" />
				<Text text="{cfl>bpName}" />
			</cells>
		</ColumnListItem>
		<columns>
			<Column>
				<header>
					<Text text="ID" />
				</header>
			</Column>
			<Column>
				<header>
					<Text text="BP Code" />
				</header>
			</Column>
			<Column>
				<header>
					<Text text="BP Name" />
				</header>
			</Column>
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>